                </or>
            </and>
            <hasName name="pytlakPtacek_*" />
        </customSelector>

        <customSelector name="isFromVidlak2" mode="or"> <!-- level 14 -->
            <and>
                <hasName name="tvid_*" />
                <or>
                    <hasFaction name="trosecko_outskirts_tanners_vidlak" />
                    <hasFaction name="trosecko_outskirts_charcoalburners_campVidlak" />
