<?xml version="1.0"?>
<!DOCTYPE storm SYSTEM "..\storm.dtd">
<storm>
	<rules>

		<rule name="inventory_player_henry" Mode="and">

			<selectors>
				<hasName name="player_henry"/>
			</selectors>

			<operations>
				<setInventory preset="inventory_player_henry" />
			</operations>

		</rule>

		<rule name="inventory_cutscene_really_naked_henry" Mode="and">

			<selectors>
				<hasName name="cutscene_really_naked_henry"/>
			</selectors>

			<operations>
				<setInventory preset="inventory_empty" />
			</operations>

		</rule>



		<rule name="inventory_player_bohuta" Mode="and">

			<selectors>
				<hasName name="player_bohuta"/>
			</selectors>

			<operations>
				<setInventory preset="inventory_player_bohuta" />
			</operations>

		</rule>

	</rules>
</storm>
