<?xml version="1.0"?>
<!DOCTYPE storm SYSTEM "..\storm.dtd">
<storm>
	<rules>

	<rule name="inventory_magickySip_karelSip">
		<selectors>
			<hasName Name="magickySip_karelSip" />
		</selectors>
		<operations>
			<setInventory preset="inventory_magickySip_karel" />
		</operations>
	</rule>
	
	<rule name="inventory_magickySip_pecha">
		<selectors>
			<hasName Name="kgru_pecha" />
		</selectors>
		<operations>
			<setInventory preset="inventory_magickySip_pecha" />
		</operations>
	</rule>

	<rule name="inventory_magickySip_zajic">
		<selectors>
			<hasName Name="kgru_zajic" />
		</selectors>
		<operations>
			<setInventory preset="inventory_magickySip_zajic" />
		</operations>
	</rule>

	<rule name="inventory_magickySip_bandits">
		<selectors>
			<or>
				<hasName Name="kgru_man_40" />
				<hasName Name="kgru_man_38" />
				<hasName Name="kgru_man_39" />
				<hasName Name="kgru_man_37" />
				<hasName Name="kgru_man_36" />
			</or>
		</selectors>
		<operations>
			<setInventory preset="inventory_magickySip_bandits" />
		</operations>
	</rule>

	</rules>
</storm>
