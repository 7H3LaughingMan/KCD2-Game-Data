<?xml version="1.0"?>
<!DOCTYPE storm SYSTEM "..\storm.dtd">
<storm>
	<rules>

		<rule name="taborOdboje_inventory_vydra">
			<selectors>
				<hasName Name="taborOdboje_vydra" />
			</selectors>
			<operations>
				<setInventory preset="inventory_vydra" />
			</operations>
		</rule>

		<rule name="taborOdboje_inventory_deadRebels_1">
			<selectors>
				<or>
					<hasName Name="taborOdboje_deadBody_1" />
					<hasName Name="taborOdboje_deadBody_5" />
				</or>
			</selectors>
			<operations>
				<setInventory preset="inventory_villager_rebel_1" />
			</operations>
		</rule>

		<rule name="taborOdboje_inventory_deadRebels_2">
			<selectors>
				<or>
					<hasName Name="taborOdboje_deadBody_2" />
					<hasName Name="taborOdboje_deadBody_4" />
				</or>
			</selectors>
			<operations>
				<setInventory preset="inventory_villager_rebel_2" />
			</operations>
		</rule>

		<rule name="taborOdboje_inventory_deadRebels_3">
			<selectors>
				<hasName Name="taborOdboje_deadBody_3" />
			</selectors>
			<operations>
				<setInventory preset="inventory_villager_rebel_3" />
			</operations>
		</rule>

		<rule name="taborOdboje_inventory_krystof_key">
			<selectors>
				<hasName Name="taborOdboje_deadBody_5" />
			</selectors>
			<operations>
				<setInventory preset="inventory_rebel_keymaster" />
			</operations>
		</rule>

	</rules>
</storm>
