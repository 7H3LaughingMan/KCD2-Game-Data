<?xml version="1.0"?>
<!DOCTYPE storm SYSTEM "..\storm.dtd">
<storm>
	<rules>

		<rule name="inventory_listovniTajemstvi_kvetoslav" Mode="and">
			<selectors>
				<hasName Name="listovniTajemstvi_kvetoslav" />
			</selectors>
			<operations>
				<setInventory preset="inventory_listovniTajemstvi_kvetoslav" />
			</operations>
		</rule>

        <rule name="inventory_listovniTajemstvi_myslibor" Mode="and">
			<selectors>
				<hasName Name="ksus_myslibor" />
			</selectors>

			<operations>
				<setInventory preset="inventory_listovniTajemstvi_myslibor" />
			</operations>
		</rule>

        <rule name="inventory_listovniTajemstvi_ranek" Mode="and">
			<selectors>
				<hasName Name="ksus_ranek" />
			</selectors>

			<operations>
				<setInventory preset="inventory_listovniTajemstvi_ranek" />
			</operations>
		</rule>

        <rule name="inventory_listovniTajemstvi_miners" Mode="and">
			<selectors>
				<hasName Name="ksus_miner_*" />
			</selectors>

			<operations>
				<setInventory preset="inventory_listovniTajemstvi_miners" />
			</operations>
		</rule>

	</rules>
</storm>
