<?xml version="1.0"?>
<!DOCTYPE storm SYSTEM "..\storm.dtd">
<storm>
	<rules>
		<rule name="hledaniPsa_corpseNaked" Mode="and">
			<selectors>
				<hasName Name="hledaniPsa_deadBody" />
			</selectors>

			<operations>
				<setInventory preset="inventory_hledaniPsa_corpseNaked" />
			</operations>
		</rule>
		<rule name="hledaniPsa_corpseRobberInventory" Mode="and">
			<selectors>
				<hasName Name="hledaniPsa_corpseRobber" />
			</selectors>

			<operations>
				<setInventory preset="inventory_hledaniPsa_corpseRobber" />
			</operations>
		</rule>
	</rules>
</storm>
