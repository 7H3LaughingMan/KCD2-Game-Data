<?xml version="1.0"?>
<!DOCTYPE storm SYSTEM "..\storm.dtd">
<storm>
	<rules>

		<rule name="outfitting_isUniqueCharacter" Mode="and">

			<selectors>
				<hasUniqueClothing/>
			</selectors>

			<operations>
				<setEquipDirectives onlyDefault="1" undressToInventory="1"/>
			</operations>

		</rule>

	</rules>
</storm>