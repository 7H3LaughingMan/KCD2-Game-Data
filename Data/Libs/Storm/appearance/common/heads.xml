<?xml version="1.0"?>
<!DOCTYPE storm SYSTEM "storm.dtd">
<storm>
	<rules>

		<rule name="head_man">
			<selectors>
				<isMan />
			</selectors>
			<operations>
				<setHead name="m_head_0*" />
			</operations>
		</rule>

		<rule name="head_woman">
			<selectors>
				<isWoman />
			</selectors>
			<operations>
				<setHead name="f_head_0*" />
			</operations>
		</rule>
	</rules>
</storm>
