<?xml version="1.0"?>
<!DOCTYPE storm SYSTEM "..\storm.dtd">
<storm>
	<rules>	
		<rule name="name_oblehaniSuchdole_defenders">
			<selectors>
				<or>
					<hasName name="zoufalaObranaZaBohutu_backWallShooterDefender*" />
					<hasName name="zoufalaObranaZaBohutu_defenders*" />				
				</or>
				<not>
					<hasName name="zoufalaObranaZaBohutu_defenders_scaredCivilian" />
				</not>
			</selectors>
			<operations>
				<setUiName name="soul_ui_name_q_suchdol_defender" />
			</operations>
		</rule>

		<rule name="name_oblehaniSuchdole_attackers">
			<selectors>
				<or>
					<hasName name="zoufalaObranaZaBohutu_attackers*" />
				</or>
			</selectors>
			<operations>
				<setUiName name="soul_ui_name_q_nebakov_attacker" />
			</operations>
		</rule>
    </rules>
</storm>    