<?xml version="1.0"?>
<!DOCTYPE storm SYSTEM "..\storm.dtd">
<storm>
	<rules>	
		<rule name="name_pogrom_enemies">
			<selectors>
					<hasName name="pogrom_enemyPubLooter*" />
			</selectors>
			<operations>
				<setUiName name="soul_ui_name_q_pogrom_enemy" />
			</operations>
		</rule>

		<rule name="name_pogrom_defenders">
			<selectors>
					<hasName name="pogrom_injuredInFinalPart1" />
			</selectors>
			<operations>
				<setUiName name="soul_ui_name_q_pogrom_defender" />
			</operations>
		</rule>

		<rule name="name_pogrom_injuredOnStreet">
			<selectors>
					<hasName name="pogrom_injuredOnStreet" />
			</selectors>
			<operations>
				<setUiName name="soul_ui_name_townsman_jewish" />
			</operations>
		</rule>

    </rules>
</storm>    