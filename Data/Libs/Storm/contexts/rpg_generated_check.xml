<?xml version="1.0"?>
<!DOCTYPE storm SYSTEM "storm.dtd">
<storm>
	<rules>
    <rule name="rpg_NOT_GENERATED">
			<selectors>
        <and>
          <isValidNpc/>    
          <not>
            <or>
              <isFromTroseckoZone1 />
              <isFromTroseckoZone2 />
              <isFromTroseckoZone3 />
              <isFromTroseckoZone4 />
              <isFromKutnohorskoZone1 />
              <isFromKutnohorskoZone2 />
              <isFromKutnohorskoZone3 />
              <isFromKutnohorskoZone4 />
              <isFromKutnaHoraZone />
            </or>
          </not>
        </and>
			</selectors>
			<operations>
				<addContext name="RPG_NOT_GENERATED_VIA_SCRIPT" />
			</operations>
		</rule>
  </rules>
</storm>
