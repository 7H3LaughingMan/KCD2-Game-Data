<?xml version="1.0"?>
<!DOCTYPE storm SYSTEM "..\storm.dtd">
<storm>
	<rules>
	
		<rule name="korenarka_dog">
			<selectors>
				<hasName name="korenarka_dog" />
			</selectors>		
			<operations>
				<setAttribute stat="agility" value="10" />
				<setAttribute stat="courage" value="10" />
				<setAttribute stat="strength" value="9" />
				<setAttribute stat="vitality" value="8" />
			</operations>
		</rule>
		
	</rules>
</storm>