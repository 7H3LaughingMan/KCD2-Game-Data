<?xml version="1.0"?>
<!DOCTYPE storm SYSTEM "..\storm.dtd">
<storm>
	<rules>

		<rule name="extraGuards_Trosky">
		
			<selectors>
				<hasName name="extraGuards_trosky_soldiers_guards_*" /> 
			</selectors>
		
			<operations>
				<setAttribute stat="combatLevel" value="0.8" />
			</operations>
		
		</rule>

		<rule name="extraGuards_KutnaHora">
		
			<selectors>
				<hasName name="extraGuards_kutnaHora_soldiers_guards_*" /> 
			</selectors>
		
			<operations>
				<setAttribute stat="combatLevel" value="0.9" />
			</operations>
		
		</rule>
		
	</rules>
</storm>