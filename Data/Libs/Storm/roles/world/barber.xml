				<isValidOpenworldNpc />
				<or>
					<hasName name="tzel_fiala" />
					<hasName name="kkut_betasBarber" />
					<hasName name="kkut_adamsBarber" />
				</or>
			</selectors>
			<operations>
				<addRole name="BARBER" />
				<addRole name="BARBER_ZDRAVI_JINDRU" />
				<addRole name="BARBER_SLUZBA_ZAPLACENA" />
				<addRole name="BARBER_SLUZBA_ZRUSENA" />
			</operations>
		</rule>
	</rules>
</storm>