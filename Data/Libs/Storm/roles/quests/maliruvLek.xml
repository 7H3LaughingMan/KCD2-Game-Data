<?xml version="1.0"?>
<!DOCTYPE storm SYSTEM "..\storm.dtd">
<storm>
	<rules>
		<rule name="maliruvLek_malir">
			<selectors>
				<hasName name="ttro_malir" />
			</selectors>
			<operations>
				<addRole name="MALIR" />
			</operations>
		</rule>
		<rule name="maliruvLek_dezerterVudce">
			<selectors>
				<hasName name="kvrc_deserterLeader"/>
			</selectors>
			<operations>
				<addRole name="VUDCE_DEZERTERU_MALIRUV_LEK"/>
				<addRole name="VYJEDNAVANI_MUZ"/>
			</operations>
		</rule>
		<rule name="maliruvLek_ksed_gravedigger">
			<selectors>
				<hasName name="ksed_gravedigger" />
			</selectors>
			<operations>
				<addRole name="HROBNIK_SEDLEC" />
				<addRole name="VYJEDNAVANI_MUZ"/>
			</operations>
		</rule>
    </rules>
</storm>