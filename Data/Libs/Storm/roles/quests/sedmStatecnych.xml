<?xml version="1.0"?>
<!DOCTYPE storm SYSTEM "!storm.dtd">
<storm>
	<rules>
	
		<rule name="sedmStatecnych_zizka">
			<selectors>
				<hasName name="tneb_zizka" />
			</selectors>
			<operations>
				<addRole name="ZIZKA_VYPRAVI_SEDM_STATECNYCH" />
			</operations>
		</rule>		

		<rule name="sedmStatecnych_kubenka">
			<selectors>
				<hasName name="kcer_kubenka" />
			</selectors>
			<operations>
				<addRole name="KUBENKA" />
			</operations>
		</rule>		
	
		<rule name="sedmStatecnych_cert">
			<selectors>
				<hasName name="kcer_suchyCert" />
			</selectors>
			<operations>
				<addRole name="SUCHY_CERT" />
			</operations>
		</rule>	
		
		<rule name="sedmStatecnych_velitel">
			<selectors>
				<hasName name="sedmStatecnych_assaultEnemy_1" />
			</selectors>
			<operations>
				<addRole name="VELITEL_KONVOJE" />
			</operations>
		</rule>	
		
		<rule name="sedmStatecnych_hospodsky">
			<selectors>
				<hasName name="kcer_innkeeper" />
			</selectors>
			<operations>
				<addRole name="HOSPODSKY_CERTOVKA" />
			</operations>
		</rule>	
		
		<rule name="sedmStatecnych_hospodsky">
			<selectors>
				<hasName name="sedmStatecnych_assaultAlly_1" />
			</selectors>
			<operations>
				<addRole name="VUDCE_VETERANU" />
			</operations>
		</rule>	
		
    </rules>
</storm>

