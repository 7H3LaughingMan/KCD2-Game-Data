				<hasName name="malir_diceman" />
			</selectors>
			<operations>
				<addRole name="KUBA_STISTKO" />
			</operations>
		</rule>
		
		<rule name="malir_bowman">
			<selectors>
				<hasName name="malir_bowman" />
			</selectors>
			<operations>
				<addRole name="JURA_VOSTRY_VOKO" />
			</operations>
		</rule>
		
		<rule name="malir_vlk">
			<selectors>
				<hasName name="malir_vlk" />
			</selectors>
			<operations>
				<addRole name="MALIR_VLK" />
			</operations>
		</rule>
    </rules>
</storm>