		</rule>		
		<rule name="kajicniPout_kutnohorsko_priest_tobias">
			<selectors>
				<hasName name="kkut_tobias" />
			</selectors>
			<operations>
				<addRole name="OTEC_TOBIAS" />
			</operations>
		</rule>								
    </rules>
</storm>