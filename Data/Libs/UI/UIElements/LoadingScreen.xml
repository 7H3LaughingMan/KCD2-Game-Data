<UIElements name="Common">

  <UIElement name="LoadingScreen" >

    <GFx file="LoadingScreen.gfx" layer="50">
      <Constraints>
        <Align mode="fullscreen" scale="1" />
      </Constraints>
    </GFx>

    <variables>
    </variables>

    <functions>
      <function name="PlayLoadingScreen" funcname="fc_playLoadingScreen" desc="">
        <param name="levelId" desc="id from table level, column loading screen id" type="int"/>
      </function>

      <function name="ShowWatermark" funcname="fc_showWatermark">
        <param name="UserId" desc="" type="string" />
      </function>
    </functions>

    <events>
      <event name="CanStop" fscommand="canStop" desc="" />
    </events>

  </UIElement>

</UIElements>
